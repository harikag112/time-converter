import java.util.Scanner;

public class Program {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int days =2;
		
		int hours=60;
		int mins=60;
		 System.out.println("Results : ");
    days=scanner.nextInt();
    System.out.println(""+(days*24*hours*mins));

		
	}
}
